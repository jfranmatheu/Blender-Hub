QMainWindow {
    background-color: #363636;
}

QPushButton {
    background-color: transparent;
    border-style: outset;
    border-width: 2px;
    border-radius: 10px;
    border-color: transparent;
    font: bold 14px;
    min-width: 10em;
    padding: 6px;
}
QPushButton:pressed {
    background-color: rgb(224, 0, 0);
    border-style: inset;
}
QPushButton:hover {
    background-color: #A8A8A8;
}